{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "lib": [
      "es7",
      "es2015",
      "dom"
    ],
    "allowJs": true,
    "outDir": "./dist/",
    "strict": true,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  }
}